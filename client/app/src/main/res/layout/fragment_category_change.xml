<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CategoryChangeFragment">


    <LinearLayout
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:gravity="end">

        <ImageButton
            android:id="@+id/updateCategory_back_button"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="20dp"
            android:src="@drawable/ic_close"
            android:background="@color/white"/>
    </LinearLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/topBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/header"
        android:layout_marginTop="40dp"
        >
        <TextView
            android:id="@+id/category_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/spoqahansansneo_bold"
            android:text="카테고리"
            android:textColor="#0A0707"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/searchView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:textStyle="bold" />

        <androidx.appcompat.widget.SearchView
            android:id="@+id/searchView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/searchview"
            app:layout_constraintTop_toBottomOf="@id/category_txt"
            tools:layout_editor_absoluteX="20dp"
            tools:layout_editor_absoluteY="128dp"
            android:layout_marginBottom="20dp"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/topBar"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/category"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@id/topBar"
            >

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/cafe_snack_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="22dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_cafe"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="카페∙간식"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/medical_snack_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_health"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="의료∙건강"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/cafe_snack_btn"
                app:layout_constraintTop_toTopOf="@id/cafe_snack_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/financial_insurance_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_finance"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="금융∙보험"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/medical_snack_btn"
                app:layout_constraintTop_toTopOf="@id/medical_snack_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/department_mall_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_department"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="백화점∙몰"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="@id/cafe_snack_btn"
                app:layout_constraintTop_toBottomOf="@id/cafe_snack_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/onlineShopping_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_cafe"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="온라인 쇼핑"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/department_mall_btn"
                app:layout_constraintTop_toTopOf="@id/department_mall_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/food_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_food"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="식비"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/onlineShopping_btn"
                app:layout_constraintTop_toTopOf="@id/onlineShopping_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/culture_art_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_art"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="문화∙예술"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="@id/department_mall_btn"
                app:layout_constraintTop_toBottomOf="@id/department_mall_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/traffic_car_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_traffic"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="교통∙자동차"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/culture_art_btn"
                app:layout_constraintTop_toTopOf="@id/culture_art_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/present_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_present"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="선물"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/traffic_car_btn"
                app:layout_constraintTop_toTopOf="@id/traffic_car_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/education_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_education"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="교육"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="@id/department_mall_btn"
                app:layout_constraintTop_toBottomOf="@id/culture_art_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/convenienceStore_mart_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_market"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="편의점∙마트"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/education_btn"
                app:layout_constraintTop_toTopOf="@id/education_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/beauty_salon_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_beauty"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="뷰티∙미용"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/convenienceStore_mart_btn"
                app:layout_constraintTop_toBottomOf="@id/present_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/assetMovement_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_assetmovement"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="자산이동"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="@id/department_mall_btn"
                app:layout_constraintTop_toBottomOf="@id/education_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/other_expense_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_others"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="기타지출"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/assetMovement_btn"
                app:layout_constraintTop_toTopOf="@id/assetMovement_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/saving_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_saving"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="저축"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/other_expense_btn"
                app:layout_constraintTop_toTopOf="@id/other_expense_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/residential_communication_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_living"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="주거∙통신"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="@id/department_mall_btn"
                app:layout_constraintTop_toBottomOf="@id/assetMovement_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/travel_accommodation_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_travel"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="여행∙숙박"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/residential_communication_btn"
                app:layout_constraintTop_toTopOf="@id/residential_communication_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/familyEvent_fee_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_dues"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="경조사∙회비"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/travel_accommodation_btn"
                app:layout_constraintTop_toTopOf="@id/travel_accommodation_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/sport_leisure_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_sport"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="스포츠∙레저"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="@id/department_mall_btn"
                app:layout_constraintTop_toBottomOf="@id/residential_communication_btn" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/alchol_nightlife_btn"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/category_icon_style"
                android:drawableStart="@drawable/ic_category_entertainment"
                android:drawablePadding="8dp"
                android:fontFamily="@font/spoqahansansneo_regular"
                android:paddingStart="12dp"
                android:paddingTop="8dp"
                android:paddingEnd="12dp"
                android:paddingBottom="8dp"
                android:stateListAnimator="@null"
                android:text="술∙유흥"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@id/sport_leisure_btn"
                app:layout_constraintTop_toTopOf="@id/sport_leisure_btn" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/newCategoryRegistration_btn"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginStart="206dp"
        android:layout_marginTop="622dp"
        android:background="@drawable/newcategoryregistration_btn"
        android:drawableStart="@drawable/ic_category_add"
        android:drawablePadding="8dp"
        android:paddingStart="8dp"
        android:paddingTop="8dp"
        android:paddingEnd="8dp"
        android:paddingBottom="8dp"
        android:text="새 카테고리 등록"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:stateListAnimator="@null"/>

</androidx.constraintlayout.widget.ConstraintLayout>